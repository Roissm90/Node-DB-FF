{
  "version": 2,
  "builds": [
    {
      "src": "index.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/usersFF/(.*)",
      "dest": "/usersFF/$1",
      "methods": ["GET", "POST"],
      "headers": {
        "Access-Control-Allow-Origin": "https://localhost:5173",
        "Access-Control-Allow-Credentials": "true"
      }
    },
    {
      "src": "/(.*)",
      "dest": "/"
    }
  ]
}
